{
    "name": "rpdesignerfly/laravel-form-helpers",
    "description": "Helpers adicionais para o desenvolvimento de formulÃ¡rios com Laravel e Blade templates",
    "keywords": ["laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Pereira",
            "email": "rpdesignerfly@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelFormHelpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFormHelpers\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "LaravelFormHelpers\\ServiceProvider"
            ],
            "aliases": {
                "FormHelper": "LaravelFormHelpers\\Facade"
            }
        }
    },
    "require-dev": {
        "illuminate/framework": "5.5.x"
    }
}
